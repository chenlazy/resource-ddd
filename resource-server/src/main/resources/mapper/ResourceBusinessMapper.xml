<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.idanchuang.resource.server.infrastructure.persistence.mapper.ResourceBusinessMapper">



    <select id="getResourceBusiness"
            resultType="com.idanchuang.resource.server.infrastructure.persistence.model.ResourceBusinessDO">
        SELECT id,business_name
        FROM `resource_business`
        WHERE is_deleted = 0
    </select>
</mapper>
